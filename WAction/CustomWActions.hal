procedure LookUpLastOpenIVCashWithItems(var LongInt lastOpenIVCasNr)
begin
  record IVCashVc IVCashr;
  Boolean TrHs,testf;
  
    lastOpenIVCasNr = -1;
    IVCashr.OKFlag = 0;
    TrHs = true;
    while (LoopBackKey("OKFlag",IVCashr,1,TrHs)) begin
      testf = true;
      if (IVCashr.OKFlag!=0) then begin TrHs = false; testf = false; end;
