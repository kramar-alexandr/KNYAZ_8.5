              PutWindowString(wn2,"CustomerDisplayData_Line2",USetStr(50010)); 
              openNewF = false;
            end;
          end;
          if (openNewF) then begin
            WINDOWDONEW(wn,0);
          end;
        end;
      otherwise
        wn = OpenWindow("NPTSIVCashLClass",1,0,"","",IVCashr);
        openNewF = true;
        LookUpLastOpenIVCashWithItems(lastOpenIVCasNr);
        if (lastOpenIVCasNr>-1) then begin
